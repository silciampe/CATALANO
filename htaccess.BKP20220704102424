<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteRule ^(.*)$ index.php [QSA,L]
    RewriteCond %{HTTP_HOST} catalano\.com\.ar [NC]
	RewriteCond %{SERVER_PORT} 80
	RewriteRule ^(.*)$ https://www.catalano.com.ar/$1 [R,L]
</IfModule>
RewriteCond %{HTTP_HOST} ^catalanojec\.com\.ar$ [OR]
RewriteCond %{HTTP_HOST} ^www\.catalanojec\.com\.ar$
RewriteRule ^/?$ "http\:\/\/www\.catalano\.com\.ar" [R=301,L]
